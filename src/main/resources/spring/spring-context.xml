<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-4.0.xsd"
       >

    <bean name="person" class="com.example.other.test01.Chinese">
        <property name="account" ref="account"></property>
        <property name="userName" value="xiaoming"></property>
    </bean>

    <bean name="account" class="com.example.other.test01.Account"></bean>

    <bean name="japanese" class="com.example.other.test01.Japanese" scope="prototype"/>

</beans>